      "Factorial 120\n"
     ]
    }
   ],
   "source": [
    "number = int(input('Enter:'))\n",
    "f=1\n",
    "i=1\n",
    "while i<= number:\n",
    "    f=f*i\n",
    "    i=i+1\n",
    "print('Factorial',f)"
   ]
